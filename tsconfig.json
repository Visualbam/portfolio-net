{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "module": "es6",
        "moduleResolution": "node",
        "target": "es5",
        "sourceMap": true,
        "skipDefaultLibCheck": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "types": [
            "webpack-env"
        ]
    },
    "exclude": [
        "bin",
        "node_modules"
    ]
}
